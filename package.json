{
  "name": "letterjam",
  "version": "0.0.0",
  "description": "Digital conversion of Letter Jam board game",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack/webpack.prod.js",
    "bundle": "npm run build",
    "startDev": "npm run start",
    "start": "./run_local.sh",
    "startProd": "NODE_ENV=production ./node_modules/.bin/ts-node src/server/server.ts",
    "lint": "eslint . --ext .ts",
    "test": "jest --testPathIgnorePatterns src/client",
    "testw": "jest --watch --testPathIgnorePatterns src/client"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.1.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.2",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "3.0.3",
    "eslint-plugin-react": "7.19.0",
    "html-webpack-plugin": "4.0.0-beta.11",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "open-cli": "^6.0.1",
    "organize-imports-cli": "^0.7.0",
    "prettier": "2.0.5",
    "ts-jest": "^26.4.0",
    "ts-loader": "^5.4.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2",
    "webpack-obfuscator": "^0.27.4",
    "workbox-webpack-plugin": "^5.1.3"
  },
  "dependencies": {
    "@babel/core": "7.9.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@svgr/webpack": "4.3.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/express-session": "^1.17.0",
    "@types/express-socket.io-session": "^1.3.2",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.161",
    "@types/node": "^12.12.67",
    "@types/react": "^16.9.52",
    "@types/react-dom": "^16.9.8",
    "@types/socket.io": "^2.1.10",
    "@types/socket.io-client": "^1.4.34",
    "css-loader": "3.4.2",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-session-sqlite": "^2.0.6",
    "express-socket.io-session": "^1.3.5",
    "lodash": "^4.17.20",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
