    "url-loader": "2.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "organize-imports-cli",
      "eslint --fix",
      "prettier --write"
